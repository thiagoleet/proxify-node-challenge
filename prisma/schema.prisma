generator client {
  provider      = "prisma-client-js"
  binaryTargets = "darwin-arm64"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  role     String
}
